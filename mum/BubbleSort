package algo;

public class BubbleSort {
	
	static int [] A = {3,7,9,1,2,4,99,10,11};
	
	public static void main(String[] args) {
		
		
		bubbleSort(A);
		for(int x:A)
			System.out.print(x+" ");
	}

	private static void bubbleSort(int[] a) {
		
		int temp;
		for(int i=0;i<a.length;i++)
			for(int j=i;j<a.length;j++)
			if(a[i]>a[j])
				{
					temp = a[i];
					a[i]= a[j];
					a[j]= temp;
				}
			}

}
